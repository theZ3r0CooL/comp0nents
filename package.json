    "@storybook/addon-controls": "^6.5.16",
    "@storybook/addon-docs": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-measure": "^6.5.16",
    "@storybook/addon-outline": "^6.5.16",
    "@storybook/addon-toolbars": "^6.5.16",
    "@storybook/addon-viewport": "^6.5.16",
    "@storybook/addons": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/node-logger": "^6.5.16",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/theming": "^6.5.16",
    "@svgr/webpack": "^6.5.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.15.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "eslint-config-react-app": "^7.0.1",
    "glob-parent": "^6.0.2",
    "react-scripts": "5.0.1",
    "rollup": "^3.19.1",
    "rollup-plugin-dts": "^5.2.0",
    "trim": "^1.0.1",
    "trim-newlines": "^4.0.2",
    "typescript": "^4.9.5",
    "webpack": "^5.76.1"
  },
  "overrides": {
    "@svgr/webpack": "$@svgr/webpack",
    "glob-parent": "$glob-parent",
    "trim": "$trim",
    "trim-newlines": "$trim-newlines"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "jsnext:main": "dist/esm/index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "rollup --config",
    "npm-check": "npx npm-check --config='.npmcheckrc.json' --specials=eslint",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theZ3r0CooL/comp0nents.git"
  },
  "bugs": {
    "url": "https://github.com/theZ3r0CooL/comp0nents/issues"
  },
  "homepage": "https://github.com/theZ3r0CooL/comp0nents#readme",
  "keywords": [
    "React",
    "Components",
    "Storybook",
    "TypeScript",
    "TS",
    "Library",
    "NPM"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": 0
    },
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": 0
        }
      }
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
